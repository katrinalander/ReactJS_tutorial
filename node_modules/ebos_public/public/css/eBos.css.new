/********************** BEGIN MESSGE BOX ***********************************/
.message-box {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    width: 200px;
    min-height: 100px;
    background-color: #fff;
    color: rgba(0, 0, 0, 0.6);
    border: 1px solid #bbb;
    border-radius: 4px;
    cursor: default;
    box-shadow: 0 6px 12px rgba(0,0,0,.175);
}

.message-container {
    min-height: 100px;
    display: table;
    overflow: hidden;
    margin: 0 auto;
    padding: 20px;
}

.message {
    display: table-cell; 
    vertical-align: middle;
    text-align: center;
    white-space: pre-wrap;
    max-width: 200px;
    line-height: 1.4;
    font-size: 12px;
}

.message-close {
    position: absolute;
    top: 0px;
    right: 0px;
    z-index: 101;
    padding: 5px 10px;
    cursor: pointer;
    font-size: 14px;
}

.message-triangle {
    position: absolute;
    top: 21px;
    left: 0;
    z-index: 102;
    height: 0;
    width: 0;
    border-bottom: 10px solid white;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
}

.message-bar {
    background-color: #dedede;
    height: 25px;
    border-bottom: 1px solid #bbb;
}
