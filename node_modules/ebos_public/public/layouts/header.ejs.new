		<script type="text/javascript">
			$(function() {
				// Begin Message Box
				function showMessageBox(id, message, xOffset, yOffset){
					$(id).css({position: 'relative'});
					$('.message-box').remove();
					$('<div class="message-triangle"></div><div class="message-box"><i class="fa fa-times message-close"></i><div class="message-container"><div class="message">' + message + '</div></div></div>').appendTo(id);
					$('.message-box').css({left: xOffset, top: yOffset});
				}

				$('.badge').on('click', function(){
					if ($('.message-box').is(':visible')) {
						$('.message-box, .message-triangle').hide();
					} else {
						// showMessageBox parameters are in order: id, message, xOffset, yOffset
						showMessageBox(this, 'This is a test message. This is a test message. This is a test message. This is a test message. This is a test message. This is a test message.', -140, 30);
					}
				});				
				//End Message Box	
			});
		</script>
